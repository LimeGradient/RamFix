{
    "manifest_version": 3,
    "name": "RamFix",
    "description": "Optimizes Google for the average person with 200 tabs open",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "host_permissions": [
        "https://developer.chrome.com/*"
      ],
    "permissions": [
        "bookmarks", "tabs"
    ]
  }